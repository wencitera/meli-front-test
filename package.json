{
  "scripts": {
    "build:server": "babel src --out-dir dist && cp -r src/server/views dist/server/views",
    "build:client": "webpack",
    "build": "npm run build:client & npm run build:server",
    "start:server": "nodemon --exec babel-node src/server/index.js",
    "start:client": "webpack --watch",
    "start": "npm-run-all --parallel start:*",
    "clean": "rm -rf node_modules package-lock.json && npm i"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-scss": "^1.2.0",
    "css-loader": "^7.1.2",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "uuid": "^10.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
